{
  "name": "nutribox",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "build:local": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "size": "node --max_old_space_size=4096",
    "fix:prettier": "prettier --write . '!.next' '!.node_modules'",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "pretty": "prettier --write . --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx . --ignore-path .gitignore",
    "formatter": "run-s pretty lint:fix",
    "typeorm": "ts-node -r dotenv/config -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "schema:drop": "npm run typeorm schema:drop",
    "seed:config": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "db:reset": "run-s schema:drop seed:run"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.106",
    "@mui/material": "^5.10.12",
    "@mui/styled-engine": "^5.11.11",
    "@mui/x-date-pickers": "^5.0.6",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-query-devtools": "^4.24.12",
    "apexcharts": "^3.44.0",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "formik": "^2.4.5",
    "http-status-codes": "^2.2.0",
    "i18next": "^22.0.6",
    "imagekit": "^4.1.2",
    "imagekitio-react": "^2.0.0",
    "imask": "^6.4.3",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "merge": "^2.1.1",
    "next": "^13.5.4",
    "next-auth": "^4.23.2",
    "next-connect": "^0.13.0",
    "next-i18next": "^13.0.0",
    "nodemailer": "^6.9.5",
    "notistack": "^3.0.1",
    "nprogress": "^0.2.0",
    "pg": "^8.11.3",
    "photoswipe": "^5.3.6",
    "pluralize": "^8.0.0",
    "pure-react-carousel": "^1.30.1",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^12.0.0",
    "react-imask": "^6.4.3",
    "react-number-format": "^5.3.1",
    "react-photoswipe-gallery": "^2.2.6",
    "react-redux": "^8.1.3",
    "react-scroll": "^1.8.8",
    "react-use": "^17.4.0",
    "sharp": "^0.31.1",
    "simplebar-react": "^2.4.3",
    "stylis": "^4.1.3",
    "stylis-plugin-rtl": "^2.1.1",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.45",
    "typewriter-effect": "^2.19.0",
    "usehooks-ts": "^2.9.1",
    "validator": "^13.11.0",
    "yup": "^0.32.11",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/crypto-js": "^4.1.2",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.3",
    "@types/nodemailer": "^6.4.11",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.24",
    "@types/react-scroll": "^1.8.4",
    "@types/stylis": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-plugin-import": "^1.13.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsconfig-paths": "^4.2.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^5.2.2"
  }
